
<div class="text-center">
    <h1 class="display-4">Rooms</h1>
    <p>Join to a room</p>
    <div class="container">
        <div class="row justify-content-md-center">
            <div class="col-4">
                <div class="list-group">
                    @foreach (var item in Challenge.Controllers.ChatController.Rooms)
                    {
                        <a class="list-group-item list-group-item-action" href="~/Chat/Room?room=@item.Key">@item.Value</a>
                    }
                </div>
            </div>
        </div>
    </div>

</div>